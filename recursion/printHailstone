void printHailstone(int number)
{
    /*  
     * STUDENT ANSWER
     */
     if(number==1){
         cout<<number;
         return;
     }
     else{
         cout<<number<<" ";
     }
     if(number%2==0){
         printHailstone(number/2);
     }
     else{
         printHailstone(number*3+1);
     }
}
